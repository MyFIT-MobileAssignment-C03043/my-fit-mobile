{
  "expo": {
    "name": "MyFIT",
    "slug": "my-fit-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myfitmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.myfitorganization.myfitmobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "notification": {
      "icon": "./assets/notifications/notification-icon.png",
      "color": "#EBFFCA"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#F7FFEA"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.myfitorganization.myfitmobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F7FFEA"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "my-fit-mobile",
          "organization": "ho-chi-minh-city-university-fq"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "38d4b196-7ea4-420f-a6ee-dd716088e250"
      }
    },
    "owner": "my-fit-organization",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/38d4b196-7ea4-420f-a6ee-dd716088e250"
    }
  }
}
